-- player select

function player_select_init()
	player_id = 0
	room_id = flr(rnd(256))
end

function player_select_update()
	if (btn(‚ùé)) player_id=1
	if (btn(üÖæÔ∏è)) player_id=2
	
	if (btnp(‚¨ÜÔ∏è)) room_id=(room_id+1)%256
	if (btnp(‚¨áÔ∏è)) room_id=(room_id-1)%256
	
	if (player_id != 0) then
	 nset("player_id", player_id)
	 nset("room_id", room_id)
		init_pong_game()
	end
end

function player_select_draw()
	print("‚ùé - player 1", 5, 10, 12)
	print("üÖæÔ∏è - player 2", 70, 10, 8)
 cursor(37, 2, 3)
	print("‚¨ÜÔ∏è room:"..room_id.." ‚¨áÔ∏è")
end


