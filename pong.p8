-- pico pong example
-- for pico socket library
-- by jesse jurman and tina howard

function _init()
 player_select_init()
end

function _update()
 if (player_id != 0) then
 	update_game()
 else 
 	player_select_update()
 end
end

function _draw()
	cls()
	map()
	if (player_id != 0) then
 	draw_game()
 else
  player_select_draw()
 end
end
