-- game logic

function init_pong_game()
	-- y position of each player
	nset("player_1_y", 58)
	nset("player_2_y", 58)
	
	-- initialize starting ball
	-- data (x, y, and speeds) 
	nset("ball_x_pos", 60)
	nset("ball_x_spd", 6)
	nset("ball_y_pos", 60)
	nset("ball_y_spd", 5)
	
	-- start the score at 0:0
	nset("score_1", 4)
	nset("score_2", 6)
end

-- paddle logic --

function update_paddles()
 player_key = "player_"..player_id.."_y"
 cur_pos = nget(player_key)
 if (btn(‚¨ÜÔ∏è)) then
  nset(
   player_key, 
   max(cur_pos-5, 9)
  )
 end
 if (btn(‚¨áÔ∏è)) then
  nset(
   player_key, 
   min(cur_pos+5, 107)
  )
 end
end

function draw_paddles()
	spr(1, 10, nget("player_1_y"), 1, 2)
	spr(2, 110, nget("player_2_y"), 1, 2)
end

-- ball logic --

function update_ball()
 
 -- update ball based on speed
 -- <5 is neg, 5 is null, 
 -- >5 is pos
 
 ninc("ball_x_pos", nget("ball_x_spd")-5)
 ninc("ball_y_pos", nget("ball_y_spd")-5)
	
	-- check if ball is in the
	-- left paddle area
	if (nget("ball_x_pos") < 16) then
	 -- see how far away the paddle
	 -- and ball is and if they are
	 -- close, swap directions
	 diff_y = abs(
	  nget("player_1_y") - 
	  nget("ball_y_pos")
	 )
		if (diff_y < 10) then
		 nset("ball_x_spd", ceil(rnd(5))+5)
		 nset("ball_y_spd", ceil(rnd(10)))
			sfx(1)
		end
	end
	
	-- check the right paddle area
	if (nget("ball_x_pos") > 103) then
	 diff_y = abs(
	  nget("player_2_y") - 
	  nget("ball_y_pos")
	 )
		if (diff_y < 10) then
		 nset("ball_x_spd", ceil(rnd(5)))
		 nset("ball_y_spd", ceil(rnd(10)))
			sfx(1)
		end
	end
	
	-- check if the ball has left
	-- on the left side
	if (nget("ball_x_pos") < 1) then
		-- bump score and reset
		ninc("score_2", 1)
		
		-- direct ball slowly right
		nset("ball_x_pos", 60)
 	nset("ball_x_spd", 6)
	 nset("ball_y_pos", 60)
	 nset("ball_y_spd", 5)
		sfx(2)
	end
	
	-- check if the ball has left
	-- on the right side
	if (nget("ball_x_pos") > 120) then
		-- bump score and reset
		ninc("score_1", 1)
		
		-- direct ball slowly left
		nset("ball_x_pos", 60)
 	nset("ball_x_spd", 4)
	 nset("ball_y_pos", 60)
	 nset("ball_y_spd", 5)
		sfx(2)
	end
	
	-- check if the ball hits the
	-- top or bottom walls
	if (nget("ball_y_pos") < 9 or 
	    nget("ball_y_pos") > 110) then
		-- bounce
		nset(
		 "ball_y_spd", 
		 abs(10-nget("ball_y_spd"))
		)
		sfx(0)
	end
end

function draw_ball()
	spr(
	 3, 
	 nget("ball_x_pos"), 
	 nget("ball_y_pos")
	)
end

-- score and room printing --

function draw_score_and_room()
 -- we need to push back the
 -- score display if it's big
 if (nget("score_1") < 10) then
 	print(nget("score_1"), 38, 1, 12)
 else
 	print(nget("score_1"), 34, 1, 12)
 end
	
	print(':', 42, 1, 0)
	print(nget("score_2"), 46, 1, 8)
	
	-- print room id
	print("room: "..nget("room_id"), 76, 1, 3)
end

-- game update and draw functions

function update_game()
	update_paddles()
	update_ball()
end

function draw_game()
	draw_paddles()
	draw_ball()
	draw_score_and_room()
end
